#include<stdio.h>
int main()
{
    float a,b,c,d,e,f;
    int n,n1,n2,n3,n4,n5,n6;
    scanf("%f%f%f%f%f%f",&a,&b,&c,&d,&e,&f);
    if(a>=0)
    n1=1;
    else
    n1=0;
    if(b>=0)
    n2=1;
    else
    n2=0;
    if(c>=0)
    n3=1;
    else
    n3=0;
    if(d>=0)
    n4=1;
    else
    n4=0;
    if(e>=0)
    n5=1;
    else
    n5=0;
    if(f>=0)
    n6=1;
    else{
    n6=0;
    }
    n=n1+n2+n3+n4+n5+n6;
    printf("%d valores positivos\n",n);
    return 0;
}
