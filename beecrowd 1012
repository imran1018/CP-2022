#include<stdio.h>
#include<math.h>
#define pi 3.14159
int main()
{
float a,b,c,tri,cir,trap,quad,ret;
scanf("%f%f%f",&a,&b,&c);
tri = 0.5*a*c;
cir = pi*c*c;
trap = 0.5*(a+b)*c;
quad = b*b;
ret = (a*b);
printf("TRIANGULO: %.3f\n",tri);
printf("CIRCULO: %.3f\n",cir);
printf("TRAPEZIO: %.3f\n",trap);
printf("QUADRADO: %.3f\n",quad);
printf("RETANGULO: %.3f\n",ret);
return 0;
}
