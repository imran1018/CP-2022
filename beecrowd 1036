#include<stdio.h>
#include<math.h>
int main()
{
    double a,b,c,d,x1,x2;
    scanf("%lf%lf%lf",&a,&b,&c);
   d = (b*b)-(4*a*c);
    if(a==0||d<0)
        printf("Impossivel calcular\n");
    else
     {
         x1 = (-b+sqrt(d))/(2*a);
        x2 = (-b-sqrt(d))/(2*a);
         printf("R1 = %.5lf\nR2 = %.5lf\n",x1,x2);
     }
    return 0;

}
