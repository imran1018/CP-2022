#include<stdio.h>
int main()
{
    int s,e,time;
    scanf("%d%d",&s,&e);
    if(s==e)
        printf("O JOGO DUROU 24 HORA(S)\n");
    if(e>s)
    {
        time = e-s;
        printf("O JOGO DUROU %d HORA(S)\n",time);
    }
    if(s>e)
    {
        time = (24-s)+e;
        printf("O JOGO DUROU %d HORA(S)\n",time);
    }
    return 0;
}
