#include<stdio.h>
int main()
{
    int i,n;
    scanf("%d",&n);
    if(n%2==0){
        i=n+1;
        while(i<=n+12){
            printf("%d\n",i);
            i=i+2;
        }
    }
    else{
        i=n;
        while(i<=n+10){
            printf("%d\n",i);
            i=i+2;
        }
    }


    return 0;
}
